{
  "name": "socialease-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit --testTimeout=10000 --verbose",
    "test:integration": "jest tests/integration --testTimeout=30000 --verbose",
    "test:api": "jest tests/api --testTimeout=45000 --verbose",
    "test:e2e": "jest tests/e2e --testTimeout=60000 --verbose",
    "test:all": "node tests/run-tests.js",
    "test:ci": "jest tests/ci-*.test.js --testTimeout=30000 --forceExit --detectOpenHandles --maxWorkers=1 --verbose",
    "test:ci-simple": "jest tests/ci-simple.test.js --testTimeout=10000 --forceExit --detectOpenHandles --maxWorkers=1",
    "test:ci-startup": "jest tests/ci-app-startup.test.js --testTimeout=30000 --forceExit --detectOpenHandles --maxWorkers=1",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "build": "echo 'Build completed - Node.js app ready for deployment'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.2.0",
    "googleapis": "^154.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "next-auth": "^4.24.11",
    "router": "^2.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
