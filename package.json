{
  "name": "socialease-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t socialease-backend .",
    "docker:run": "docker run -p 4000:4000 socialease-backend",
    "docker:build-prod": "docker build --target production -t socialease-backend:prod .",
    "docker:run-prod": "docker run -p 4000:4000 socialease-backend:prod"
  },
  "keywords": ["social-skills", "gamification", "education", "api", "nodejs", "express", "mongodb"],
  "author": "SocialEase Team",
  "license": "MIT",
  "description": "A robust Node.js backend API for SocialEase, a social skills development platform with gamification, scenarios, and progress tracking",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.2.0",
    "googleapis": "^154.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "next-auth": "^4.24.11",
    "router": "^2.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  }
}
